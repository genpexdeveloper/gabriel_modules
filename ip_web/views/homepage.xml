  <!-- HOMEPAGE SIDEBAR TEMPLATE -->
  <template id="homepage_sidebar" name="Homepage Sidebar" inherit_id="ip_web.homepage">
   <!-- INCLUDE SIDEBAR -->
   <xpath expr="//div[@id='ip-sidebar']" position="replace">
    <!-- SIDEBAR -->
    <div id="ip-sidebar" class="col-md-3 col-xs-12 hidden-xs" >
     <div class="ip-sidebar-widget-placeholder" />
     <!-- WIDGET -->
